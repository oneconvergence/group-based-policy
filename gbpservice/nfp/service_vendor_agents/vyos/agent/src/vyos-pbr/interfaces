iface eth6 inet dhcp
post-up /etc/network/interface-post-up

allow-hotplug eth7
iface eth7 inet dhcp
post-up /etc/network/interface-post-up

allow-hotplug eth8
iface eth8 inet dhcp
post-up /etc/network/interface-post-up

allow-hotplug eth9
iface eth9 inet dhcp
post-up /etc/network/interface-post-up

